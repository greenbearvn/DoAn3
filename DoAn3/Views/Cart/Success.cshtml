
@{
    ViewBag.Title = "Success";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container" style="background-color: #FCFCFC" >

    <div class="page">

        <main class="page-content" data-cart="">
            <ul class="breadcrumbs">
                <li class="breadcrumb ">
                    <a href="https://gameworld1.mybigcommerce.com/" class="breadcrumb-label">Trang chủ</a>
                </li>
                <li class="breadcrumb is-active">
                    <a href="https://gameworld1.mybigcommerce.com/cart.php?suggest=6045dc73-eaeb-4fc1-a4e0-9fa2478691ed" class="breadcrumb-label">Thanh toán thành công</a>
                </li>
            </ul>



            <h1 class="page-heading" data-cart-page-title="">
            </h1>
            <p></p>



            <h1 class="page-heading text-4xl " >
                Thanh toán thành công
            </h1>
            <div data-cart-status="">
            </div>

            <div class="loadingOverlay" style="display: none;"></div>
            <a href="/Home/Index" style="color:dodgerblue" class="inline-block px-6 py-2.5 bg-blue-600 text-white font-medium text-xs leading-tight uppercase rounded shadow-md hover:bg-blue-700 hover:shadow-lg focus:bg-blue-700 focus:shadow-lg focus:outline-none focus:ring-0 active:bg-blue-800 active:shadow-lg transition duration-150 ease-in-out">Trở về mua sắm</a>





            <!-- snippet location cart -->
        </main>
    </div>

</div>

@section scripts{
    <script>
          var myApp = angular.module("myApp", []).config(['$qProvider', function ($qProvider) {
            $qProvider.errorOnUnhandledRejections(false);
          }]);

       

        myApp.controller("getLoaiMay", getLoaiMay);
        function getLoaiMay($scope, $http) {

            $http({
                method: "GET",
                url: "/Home/LayLoaimay"

            }).then(function (res) {
                $scope.loaimay = res.data;

            });
        }

        myApp.controller("SearchData", SearchData);
        function SearchData($scope, $http) {

            $http({
                method: "GET",
                url: "/Home/dataSearch"

            }).then(function (res) {
                $scope.datas = res.data;
            });
        }
        myApp.controller("recommendPost", recommendPost);
        function recommendPost($scope, $http) {

            $http({
                method: "GET",
                url: "/NewsFront/RecommendPost"

            }).then(function (res) {
                $scope.datas = res.data;
            });
        }

    </script>

}